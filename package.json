  "description": "A TypeScript library for constructing and utilizing AI agents with OpenAI's API, enabling the chaining of multiple requests and responses using RxJS, and generating runtime type information for the 'function_call' feature.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist.esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "ttsc -b",
    "docs": "typedoc"
  },
  "keywords": [
    "openai",
    "ai",
    "agent",
    "typescript",
    "artificial-intelligence",
    "machine-learning",
    "api",
    "ts-rtti",
    "runtime-type-information",
    "function-call",
    "gpt-3.5-turbo-0613",
    "gpt-4-0613"
  ],
  "dependencies": {
    "openai": "^3.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typescript-rtti": "^0.8.3"
  },
  "devDependencies": {
    "ttypescript": "^1.5.15"
  },
  "author": "x0rb0t",
  "license": "MIT"
}
